public class Main {
    public static void main(String[] args) {
        Pikachu p = new Pikachu();
        Bulbasaur b = new Bulbasaur ();
        Squirtle s = new Squirtle ();
        Charmander c = new Charmander ();

        p.Aranazo();
        p.punotrueno();
        b.Mordisco();
        b.Drenadoras();
        c.Aranazo();
        c.LatigoIgneo();
        s.Placaje();
        s.AcuaAro();
    }
}
